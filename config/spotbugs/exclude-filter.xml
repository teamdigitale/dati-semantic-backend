<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Package name="~.*\.gen\..*" />
    </Match>
     <!-- We're excluding the following items because they are false positives generated by
         Lombok's annotations -->
    <Match>
        <Bug pattern="EI_EXPOSE_REP"/><!-- False positive when using lombok -->
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP2"/><!-- False positive when using lombok -->
    </Match>
    <Match>
        <Bug pattern="ES_COMPARING_PARAMETER_STRING_WITH_EQ"/><!-- False positive when using lombok -->
    </Match>
    <Match>
        <Bug pattern="SS_SHOULD_BE_STATIC"/>
    </Match>
    <Match>
        <Bug pattern="CT_CONSTRUCTOR_THROW"/>
    </Match>
</FindBugsFilter>
